{
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "headers": [
    {
      "source": "/entry",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "application/json"
        },
        {
          "key" : "Access-Control-Allow-Methods",
          "value" : "POST"
        }
      ]
    },
    {
      "source": "/action",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "application/json"
        },
        {
          "key" : "Access-Control-Allow-Methods",
          "value" : "POST"
        }
      ]
    }
  ]
}
